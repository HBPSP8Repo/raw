<queries dataset="publications">
    <query>
        <oql>
            min(select year from authors)
        </oql>
        <qrawl>
            min(select a.year from a in authors)
        </qrawl>
        <result>
            1951
        </result>
    </query>

    <query>
        <oql>
            max(select year from authors)
        </oql>
        <qrawl>
            max(select a.year from a in authors)
        </qrawl>
        <result>
            1994
        </result>
    </query>

    <query>
        <oql>
            sum(select year from authors)
        </oql>
        <qrawl>
            sum(select a.year from a in authors)
        </qrawl>
        <result>
            98724
        </result>
    </query>

    <query disabled="https://github.com/raw-db/ldb/issues/25">
        <oql>
            avg(select year from authors)
        </oql>
        <qrawl>
            avg(select a.year from a in authors)
        </qrawl>
        <result>
            1974
        </result>
    </query>

    <query>
        <oql>
            count(select year from authors)
        </oql>
        <qrawl>
            count(select a.year from a in authors)
        </qrawl>
        <result>
            50
        </result>
    </query>

    <query disabled="https://github.com/raw-db/ldb/issues/25">
        <oql>
            unique(select year from authors)
        </oql>
        <qrawl>
            unique(select year from authors)
        </qrawl>
        <result>
            false
        </result>
    </query>

    <query>
        <oql>
            EXISTS (select year from authors)
        </oql>
        <qrawl>
            EXISTS (select a.year from authors a where a.year > 1950)
        </qrawl>
        <result>
            true
        </result>
    </query>
</queries>